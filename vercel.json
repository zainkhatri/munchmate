{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/get-meal",
        "dest": "app.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "/logo(.*)",
        "dest": "/logo$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }